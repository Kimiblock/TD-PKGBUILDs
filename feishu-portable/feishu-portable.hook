[Action]
When = PostTransaction
Exec = /usr/lib/feishu-portable/install-hook.sh
Depends = feishu-portable
Description = Configuring Feishu

[Trigger]
Operation = Install
Operation = Upgrade
Type = Path
Target = usr/lib/feishu-portable/*
